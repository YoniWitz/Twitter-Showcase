    "url": "https://github.com/YoniWitz/Twitter-Showcase-React/issues"
  },
  "homepage": "https://github.com/YoniWitz/Twitter-Showcase-React#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mdbreact": "^4.25.0",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "reactstrap": "^8.4.0"
